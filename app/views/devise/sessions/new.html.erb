<div class="py-3 login-f-container">
<%= form_for(resource, as: resource_name, url: session_path(resource_name),:html => {:id => "login-form-sub"}) do|f| %>
  <div class="field mt-4">
    <%= f.email_field :email, autofocus: true,placeholder:"Email@mail.com",class:"mb-2 w-100 form-boot", autocomplete: "email" %>
  </div>

  <div class="field">
    <%= f.password_field :password,placeholder:"password",class:"mb-2 w-100 form-boot", autocomplete: "current-password" %>
  </div>
<% end %>
<div class="shared-links-container">
  <%= render "devise/shared/links" %>
</div>
</div>

